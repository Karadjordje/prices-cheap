{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "indent": ["error", 4],
        "no-await-in-loop": "off",
        "no-plusplus": "off",
        "no-undef": "off"
    }
}
